<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Job_portal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Job_Portal"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.Job_portal.InsertJobPostActivity"></activity>
        <activity android:name="com.example.Job_portal.PostJobActivity" />
        <activity android:name="com.example.Job_portal.AllJobActivity" />
        <activity android:name="com.example.Job_portal.HomeActivity" />
        <activity android:name="com.example.Job_portal.ResetActivity" />
        <activity android:name="com.example.Job_portal.RegistrationActivity" />

        <activity android:name="com.example.Job_portal.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>